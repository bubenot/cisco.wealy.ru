<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8">
	<title>Cisco IOS Commands - {{ title['name'] }} - {{ subtitle['sub_name'] }}</title>
	<link rel="stylesheet" type="text/css" href="{{ url_for('static', filename='css/main.css') }}">
</head>
<body>
	<center>
		<h1>{{ title['name'] }} - {{ subtitle['sub_name'] }}</h1>
	</center>
	<center>
		{% for i in data1 %}
			<h3>{{ i['table_name'] }}</h3>
			<table border="1">
				<tbody>
					<tr>
					{% if i['default_columns'] == 1 %}
						{% for x in default_columns.values() %}
							<th>{{ x }}</th>
						{% endfor %}
					{% else %}
						{% for x in encode(i['columns']).values() %}
								<th>{{ x }}</th>
						{% endfor %}
					{% endif %}
					</tr>
					{% if i['default_columns'] == 1 %}
						{% for y in data2 %}
						{% if i['id_table'] == y['id_table'] %}
							<tr>
								{% for x in default_columns.keys() %}
								<td>{{ Markup(y[x]) }}</td>
								{% endfor %}
							</tr>
						{% endif %}
						{% endfor %}
					{% endif %}
					{% if i['comment'] != None %}
					<tr>
						<td colspan="100%" style="text-align:center">Примечание</td>
					</tr>
					<tr>
						<td colspan="100%" id="comments"></td>
					</tr>
					{% endif %}
				</tbody>
			</table>
		{%endfor%}
	</center>

	<center><h4><a href="{{ title['href'] }}">Вернуться назад</a></h4></center>
	
</body>
</html>